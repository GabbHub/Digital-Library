        <xs:element ref="ns1:s"/>
        <xs:element ref="ns1:pb"/>
        <xs:element ref="ns1:lb"/>
        <xs:element ref="ns1:anchor"/>
        <xs:element ref="ns1:note"/>
        <xs:element ref="ns1:figure"/>
        <xs:element ref="ns1:ref"/>
        <xs:element ref="ns1:ptr"/>
